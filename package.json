{
  "name": "rainte",
  "displayName": "Rainte",
  "description": "Visual Studio Code 插件",
  "repository": "https://github.com/rainte/vscode",
  "keywords": [
    "插件"
  ],
  "version": "1.0.2",
  "publisher": "hub",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.73.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hub.pic"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hub.pic",
        "title": "Hub.Pic"
      }
    ],
    "keybindings": [
      {
        "command": "hub.pic",
        "key": "ctrl+alt+e",
        "mac": "cmd+alt+e",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Rainte",
      "properties": {
        "hub.pic.input": {
          "type": "string",
          "default": "${y}/${m}/${uuid}${ext}",
          "markdownDescription": "上传名称格式.\n- `${uuid}`: UUID.\n- `${Y}`: 4 位年.\n- `${y}`: 2 位年.\n- `${m}`: 月.\n- `${d}`: 日.\n- `${h}`: 时.\n- `${i}`: 分.\n- `${s}`: 秒.\n- `${name}`: 文件名.\n- `${ext}`: 扩展名.\n\nExample: `${y}/${m}/${uuid}${ext}`\n"
        },
        "hub.pic.output": {
          "type": "string",
          "default": "![${dir}/${name}${ext}](${url})",
          "markdownDescription": "返回名称格式.\n- `${dir}`: 路径.\n- `${name}`: 文件名.\n- `${ext}`: 扩展名.\n- `${url}`: URL.\n\nExample: `![${dir}/${name}${ext}](${url})`\n"
        },
        "hub.pic.bed.current": {
          "type": "string",
          "enum": [
            "smms",
            "aliyun",
            "github",
            "imgur",
            "qiniu",
            "tcyun",
            "upyun",
            "weibo"
          ],
          "default": "qiniu",
          "markdownDescription": ""
        },
        "hub.pic.bed.smms.token": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.accessKeyId": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.accessKeySecret": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.bucket": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.area": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.path": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.aliyun.customUrl": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.github.repo": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.github.token": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.github.path": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.github.customUrl": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.github.branch": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.imgur.clientId": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.imgur.proxy": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.accessKey": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.secretKey": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.bucket": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.url": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.area": {
          "type": "string",
          "enum": [
            "z0",
            "z1",
            "z2",
            "na0",
            "as0"
          ],
          "default": "z0",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.options": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.qiniu.path": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.version": {
          "type": "string",
          "enum": [
            "v4",
            "v5"
          ],
          "default": "v5",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.secretId": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.secretKey": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.bucket": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.appId": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.area": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.path": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.tcyun.customUrl": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.bucket": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.operator": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.password": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.options": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.path": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.upyun.url": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.weibo.chooseCookie": {
          "type": "boolean",
          "default": true,
          "markdownDescription": ""
        },
        "hub.pic.bed.weibo.username": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        },
        "hub.pic.bed.weibo.quality": {
          "type": "string",
          "enum": [
            "thumbnail",
            "mw690",
            "large"
          ],
          "default": "large",
          "markdownDescription": ""
        },
        "hub.pic.bed.weibo.cookie": {
          "type": "string",
          "default": "",
          "markdownDescription": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/inquirer": "^0.0.42",
    "@types/vscode": "^1.73.1",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "dayjs": "^1.11.6",
    "emoji-regex": "^10.2.1",
    "picgo": "^1.5.0",
    "uuid": "^8.3.2"
  }
}
